(jbuild_version 1)

(executable
  ((name sigs_packer)
   (public_name tezos-protocol-environment-sigs-packer)
   (package tezos)
   (flags (:standard -w -9-32 -safe-string))))

(alias
 ((name runtest_indent)
  (deps ((glob_files *.ml) (glob_files *.mli)))
  (action (run bash ${libexec:tezos-stdlib:test-ocp-indent.sh} ${<}))))
